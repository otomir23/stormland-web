{
  "name": "stormland-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "dotenv -c -- prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "migrate:reset": "dotenv -e .env.local -- prisma migrate reset",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "prisma:generate": "dotenv -c -- prisma generate",
    "prisma:studio": "dotenv -c -- prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "argon2": "^0.30.3",
    "autoprefixer": "10.4.14",
    "cva": "npm:class-variance-authority",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "jose": "^4.14.4",
    "lucide-react": "^0.241.0",
    "msmc": "^4.1.0",
    "next": "13.4.4",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "encoding": "^0.1.13",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.15.0"
  }
}
